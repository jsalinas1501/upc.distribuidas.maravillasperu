                @Html.DisplayFor(modelItem => item.inicioreserva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.finreserva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.estado)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id1 = item.codigoreserva }) |
                @Html.ActionLink("Detalle", "Details", new { id = item.codigoreserva.Trim() }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.codigoreserva.Trim() })
            </td>
        </tr>
    }
</table>