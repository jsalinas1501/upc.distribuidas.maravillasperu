        <th>
            @ViewBag.Titulo03
        </th>
        <th>
            @ViewBag.Titulo04
        </th>
        <th>
            @ViewBag.Titulo05
        </th>
        <th>
            @ViewBag.Titulo06
        </th>
        <th>
            @ViewBag.Titulo07
        </th>
        <th>
            @ViewBag.Titulo08
        </th>
        <th>
            @ViewBag.Titulo09
        </th>

        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.codigoreserva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.codigociudadorigen)
            </td>
